{
  "version": 2,
  "name": "marketplace",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" },
    { "src": "/auth", "dest": "https://decentraland.zone/auth" }
  ],
  "build": {
    "env": {
      "GEN_STATIC_LOCAL": "true",
      "VITE_REACT_APP_DCL_DEFAULT_ENV": "dev"
    }
  },
  "github": {
    "silent": true
  }
}
